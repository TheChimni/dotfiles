alias lg="ls -Glah"
alias ll="ls -lah"
alias vim="/Applications/MacVim.app/Contents/MacOS/Vim"
alias be="bundle exec"
alias pgstart="pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start"
alias pgstop="pg_ctl -D /usr/local/var/postgres stop -s -m fast"
alias git-daemon="git daemon --base-path=/Users/stevehook/Workspace &"
alias esstart="launchctl load -wF ~/Library/LaunchAgents/homebrew.mxcl.elasticsearch.plist"
alias esstop="launchctl unload -wF ~/Library/LaunchAgents/homebrew.mxcl.elasticsearch.plist"
alias gitk="gitk --all &"
